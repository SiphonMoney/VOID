{
  "name": "anonymaus-tee-server",
  "version": "1.0.0",
  "description": "Mock TEE server simulating Intel SGX hardware for AnonyMaus",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "deploy-solana-program": "node scripts/deploy-solana-program.js",
    "initialize-solana-program": "node scripts/initialize-solana-program.js",
    "check-executor-setup": "node scripts/check-executor-setup.js",
    "build-program": "powershell -ExecutionPolicy Bypass -File build-native.ps1",
    "build-program-linux": "bash build-native.sh",
    "deploy-program": "solana program deploy programs/anonymaus-executor/target/deploy/anonymaus_executor.so"
  },
  "keywords": [
    "tee",
    "sgx",
    "anonymaus",
    "privacy"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.58",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.95.4",
    "bs58": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "tweetnacl": "^1.0.3"
  }
}
